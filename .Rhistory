knitr::opts_chunk$set(echo = TRUE)
library("raster")
elev <- getData("worldclim", var="alt", res=10)
ger <- getData('GADM', country='DEU', level=0)
crop_elev <- crop(elev, extent(ger))
clip_elev <- mask(crop_elev, ger)
plot(clip_elev)
plot(ger, add=T)
grp <- read.csv("C:/Users/super/Documents/sdm/Abgabe_3/Input/participant_locations.csv")
lola <- grp[,c(3,2)]
spdf <- SpatialPointsDataFrame(coords = lola, data = grp, proj4string = CRS("+proj=longlat +datum=WGS84 +ellps=WGS84 +towgs84=0,0,0"))
plot(clip_elev)
plot(ger, add=T)
plot(spdf, add=T)
plot(clip_elev)
plot(ger, add=T)
plot(spdf, add=T)
points(13.404954,52.520008,pch=8,cex=2)
points(9.993682,53.551086,pch=13,cex=2)
points(11.576124,48.137154,pch=13,cex=2)
plot(clip_elev)
plot(ger, add=T)
plot(spdf, add=T)
points(13.404954,52.520008,pch=8,cex=2)
points(9.993682,53.551086,pch=13,cex=2)
points(11.576124,48.137154,pch=13,cex=2)
legend("bottomright",legend = c("Residences","Berlin","Hamburg","Muenchen"),pch = c(3,8,13,13),title="Places")
